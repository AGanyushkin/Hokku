{
  "name": "hokku",
  "version": "0.0.7",
  "description": "Event-Based Development Environment",
  "main": "index.js",
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.52",
    "@babel/parser": "^7.0.0-beta.52",
    "cli-table": "^0.3.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-static": "^4.0.1",
    "koa2-cors": "^2.0.3",
    "netmask": "^1.0.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "rethinkdb": "^2.3.3",
    "rxjs": "^5.4.3",
    "shelljs": "^0.7.8",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.3.5",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chai": "3.5.0",
    "chai-dom": "^1.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.54.1",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^1.7.1",
    "karma-allure-reporter": "^1.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-edge-launcher": "^0.4.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "3.3.0",
    "mocha-allure-reporter": "^1.3.2",
    "mocha-multi": "^0.11.0",
    "node-fetch": "^1.7.3",
    "nyc": "^11.2.1",
    "puppeteer": "^1.6.0",
    "sinon": "^3.2.1",
    "stryker": "^0.9.1",
    "stryker-api": "^0.8.0",
    "stryker-karma-runner": "^0.7.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "walk": "^2.3.9",
    "webpack": "3.1.0",
    "webpack-dev-server": "^2.9.3",
    "yargs": "7.1.0"
  },
  "scripts": {
    "build": "npm run core:build && npm run node:build && npm run web:build && npm run docs:build",
    "test": "npm run core:test && npm run core:integration",
    "docs:build": "npm run docs:build:examples && npm run docs:build:common",
    "docs:build:common": "cp -rf ./docs/* ./build/docs/",
    "docs:build:examples": "node ./scripts/buildDocsExamples.js",
    "core:build": "npm run core:build:sources && npm run core:build:package",
    "core:build:sources": "webpack --progress --colors --config ./scripts/core/webpack.config.js",
    "core:build:package": "node ./scripts/buildPackage.js core",
    "core:_test:node": "mocha --compilers js:babel-core/register --colors ./test/javascript/core/**/*.spec.js",
    "core:test:node": "cross-env PLATFORM_ENV=node NODE_ENV=test nyc --report-dir ./reports/coverage/core/node -n **/lib/core/javascript/*.js -n **/lib/core/javascript/**/*.js npm run core:_test:node",
    "core:test:browser": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start ./scripts/core/karma.conf.js",
    "core:test:browsers": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start --browsers Chrome,Firefox ./scripts/core/karma.conf.js",
    "core:test:watch": "cross-env PLATFORM_ENV=node NODE_ENV=test npm run core:_test:node -- -w",
    "core:test": "npm run core:test:node && npm run core:test:browser",
    "core:integration:node": "mocha --compilers js:babel-core/register --colors ./test/integration/core/**/*.spec.js",
    "core:integration:browser": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start ./scripts/core/integration.karma.conf.js",
    "core:integration:browsers": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start --browsers Chrome,Firefox ./scripts/core/integration.karma.conf.js",
    "core:integration": "npm run core:integration:node && npm run core:integration:browser",
    "core:flow": "cd lib/core/ && flow",
    "core:show:allure": "node ./node_modules/allure-commandline/bin/allure serve ./reports/allure/core/",
    "node:build": "npm run node:build:sources && npm run node:build:package",
    "node:build:sources": "webpack --progress --colors --config ./scripts/node/webpack.config.js",
    "node:build:package": "node ./scripts/buildPackage.js node",
    "node:_test": "mocha --compilers js:babel-core/register --colors ./test/javascript/node/**/*.spec.js",
    "node:test": "cross-env PLATFORM_ENV=node NODE_ENV=test nyc --report-dir ./reports/coverage/node -n **/lib/node/javascript/*.js -n **/lib/node/javascript/**/*.js npm run node:_test",
    "node:test:watch": "cross-env PLATFORM_ENV=node NODE_ENV=test npm run node:_test -- -w",
    "node:integration": "mocha --compilers js:babel-core/register --colors ./test/integration/node/**/*.spec.js",
    "node:flow": "cd lib/node/ && flow",
    "node:show:allure": "node ./node_modules/allure-commandline/bin/allure serve ./reports/allure/node/",
    "web:build": "npm run web:build:sources && npm run web:build:package",
    "web:build:sources": "webpack --progress --colors --config ./scripts/web/webpack.config.js",
    "web:build:package": "node ./scripts/buildPackage.js web",
    "web:test": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start ./scripts/web/karma.conf.js",
    "web:test:watch": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start ./scripts/web/watch.karma.conf.js",
    "web:integration": "cross-env PLATFORM_ENV=browser NODE_ENV=test karma start ./scripts/web/integration.karma.conf.js",
    "web:flow": "cd lib/web/ && flow",
    "web:show:allure": "node ./node_modules/allure-commandline/bin/allure serve ./reports/allure/web/",
    "mock:init": "npm run mock:init:docker",
    "mock:deinit": "npm run mock:deinit:docker",
    "mock:start": "npm run mock:start:docker && npm run mock:start:mock-server",
    "mock:stop": "npm run mock:stop:docker",
    "mock:init:docker": "docker run --name hokku_integration_rdb -p 8080:8080 -p 28015:28015 -p 29015:29015  -d rethinkdb:2.3.6",
    "mock:deinit:docker": "docker rm hokku_integration_rdb",
    "mock:start:mock-server": "node ./test/integration/web/mock-server.js",
    "mock:start:docker": "docker start hokku_integration_rdb",
    "mock:stop:docker": "docker stop hokku_integration_rdb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGanyushkin/hokku.git"
  },
  "keywords": [
    "javascript",
    "event"
  ],
  "author": "Andrey Ganyushkin <aganyushkin@hotmail.com>",
  "license": "apache-2.0",
  "bugs": {
    "url": "https://github.com/AGanyushkin/hokku/issues"
  },
  "homepage": "https://github.com/AGanyushkin/hokku#README.md"
}
